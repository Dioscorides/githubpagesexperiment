{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["library", "nation", "city", "website"],
    "properties": {
      "library": { "type": "string", "minLength": 2 },
      "nation": { "type": "string" },
      "city": { "type": "string" },
      "website": { "type": "string", "format": "uri" },
      "lat": { "type": "string", "pattern": "^-?\\d+(\\.\\d+)?$" },
      "lng": { "type": "string", "pattern": "^-?\\d+(\\.\\d+)?$" },
      "iiif": { "type": "boolean" },
      "is_free_cultural_works_license": { "type": "boolean" },
      "quantity": { 
        "type": "string", 
        "enum": ["Low", "Medium", "High", "Unknown", "Hundreds", "Thousands"] 
      }
    }
  }
}
